<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias type="com.como.server.model.Board" alias="board"/>
        <typeAlias type="com.como.server.model.BoardLike" alias="board_like"/>
        <typeAlias type="com.como.server.model.Comment" alias="comment"/>
        <typeAlias type="com.como.server.model.CommentLike" alias="comment_like"/>
        <typeAlias type="com.como.server.model.Member" alias="member"/>
        <typeAlias type="com.como.server.model.SolveProblem" alias="solve_problem"/>
    </typeAliases>

</configuration>
